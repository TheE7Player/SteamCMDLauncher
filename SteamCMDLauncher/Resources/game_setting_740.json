// Game Settings for CSGO (740 - Parent 730)
{
  // Key: Tab, Value: Children/Options
  // Hashtag: For translations (Hint to find the correct string for the current language)
  "setup": {
    "target": "srcds.exe"
  },
  "#General": {
    "Server_Name": {
      "text": "#Server_Name",
      "type": "input",
      "can_leave_blank": true,
      "placeholder": "Counter-Strike: Global Offsensive",
      "width": 300,
      "command": "hostname \"$\"",
      "command_prefix": "-"
    },
    "Password_SVR": {
      "text": "#Server_Pass",
      "type": "pass",
      "can_leave_blank": true,
      "hint": "#Server_Pass_Hint",
      "width": 200,
      "command": "sv_password \"$\"",
      "command_prefix": "-",
      "alert": "#Server_Pass_Alert",
      "placeholder": "#Server_Pass_PlaceHolder"
    },
    "Server_Tick": {
      "text": "#Is_128_Tick",
      "type": "check",
      "default": false,
      "hint": "#Is_128_Tick_Hint",
      "returns": "64, 128",
      "command": "tickrate $",
      "command_prefix": "-"
    },
    "Server_Lan": {
      "text": "#Is_Lan",
      "type": "check",
      "default": false,
      "hint": "#Is_Lan_Hint",
      "returns": "0, 1",
      "command": "sv_lan $",
      "command_prefix": "-"
    },
    "Server_Vac": {
      "text": "#Is_Vac",
      "type": "check",
      "default": true,
      "hint": "#Is_Vac_Hint",
      "returns": "insecure, _",
      "command": "$",
      "command_prefix": "-"
    },
    "Server_Map": {
      "text": "#Svr-Map",
      "type": "combo",
      "can_leave_blank": false,
      "carry-dir": true,
      "combo-target": "\\csgo\\maps;*.bsp",
      "hint": "#Svr-Map-Hint",
      "command": "map \"$\"",
      "command_prefix": "-"
    }
  },
  "#Misc": {
    "Max-Players": {
      "text": "#MaxP",
      "type": "combo",
      "default": "1",
      "combo-range": "1-50", 
      "command": "maxplayers $",
      "command_prefix": "+"
    }
  }
}