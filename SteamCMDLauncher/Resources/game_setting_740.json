// Game Settings for CSGO (740 - Parent 730)
{
  // Key: Tab, Value: Children/Options
  // Hashtag: For translations (Hint to find the correct string for the current language)
  "setup": {
    "target": "srcds.exe",
    "precommands": "-game csgo -console +mapgroup mg_active -ip $IP4",
    "server_join_command": "connect $IP;password $P;"
  },
  "#General": {
    "Server_Name": {
      "text": "#Server_Name",
      "type": "input",
      "can_leave_blank": true,
      "placeholder": "Counter-Strike: Global Offensive",
      "width": 300,
      "command": "hostname $",
      "command_prefix": "+"
    },
    "Password_SVR": {
      "text": "#Server_Pass",
      "type": "pass",
      "tag": "PASS",
      "can_leave_blank": true,
      "hint": "#Server_Pass_Hint",
      "width": 200,
      "command": "sv_password $",
      "command_prefix": "+",
      "alert": "#Server_Pass_Alert",
      "placeholder": "#Server_Pass_PlaceHolder"
    },
    "Auth_Token": {
      "text": "#Server_Auth",
      "type": "pass",
      "can_leave_blank": false,
      "hint": "#Server_Auth_Hint",
      "width": 200,
      "command": "sv_setsteamaccount $",
      "command_prefix": "+",
      "alert": "#Server_Auth_Alert",
      "blank_alert": "#Server_Auth_Empty"
    },
    "Server-Port": {
      "text": "#ServerPort",
      "type": "combo",
      "tag": "PORT",
      "default": "27015",
      "combo-range": "27010-27030",
      "command": "port $",
      "command_prefix": "-"
    },
    "Server_Vac": {
      "text": "#Is_Vac",
      "type": "check",
      "default": true,
      "hint": "#Is_Vac_Hint",
      "returns": "insecure, _",
      "command": "$",
      "command_prefix": "-"
    }
  },
  "#Misc": {
    "Server_Map": {
      "text": "#Svr-Map",
      "type": "combo",
      "can_leave_blank": false,
      "carry-dir": true,
      "combo-target": "\\csgo\\maps;*.bsp",
      "hint": "#Svr-Map-Hint",
      "command": "map $",
      "command_prefix": "+"
    },
    "Server_GameMode": {
      "text": "#SVR_GameMode",
      "type": "combo",
      "default": "Casual",
      "combo-strict": {
        "Casual": "game_type 0 +game_mode 0",
        "Competitve": "game_type 0 +game_mode 1",
        "Wingman": "game_type 0 +game_mode 2",
        "Weapon Expert": "game_type 0 +game_mode 3",
        "Arms Race": "game_type 1 +game_mode 0",
        "Demolition": "game_type 1 +game_mode 1",
        "Deathmatch": "game_type 1 +game_mode 2",
        "Danger Zone": "game_type 6 +game_mode 0"
      },
      "command": "$",
      "command_prefix": "+"
    },
    "Max-Players": {
      "text": "#MaxP",
      "type": "combo",
      "default": "1",
      "combo-range": "1-50",
      "command": "maxplayers_override $",
      "command_prefix": "+"
    },
    "Server_Tick": {
      "text": "#Is_128_Tick",
      "type": "check",
      "default": false,
      "hint": "#Is_128_Tick_Hint",
      "returns": "64, 128",
      "command": "tickrate $",
      "command_prefix": "-"
    },
    "Server_Lan": {
      "text": "#Is_Lan",
      "type": "check",
      "default": false,
      "hint": "#Is_Lan_Hint",
      "returns": "0, 1",
      "command": "sv_lan $",
      "command_prefix": "-"
    }
  },
  "#SourceMod": {
    "validate-folder": [
      "\\csgo\\addons\\metamod\\sourcemod.vdf",
      "\\csgo\\addons\\sourcemod\\"
    ],
    "SM_Admins": {
      "text": "#SM-Admins",
      "type": "input",
      "can_leave_blank": true,
      "placeholder": "\"STEAM_0:1:XXXXXXXX\" \"99:z\";\"STEAM_0:1:XXXXXXXX\" \"99:z\"",
      "hint": "#SM-Admins-Hint",
      "width": 600,
      "write_to": "\\csgo\\addons\\sourcemod\\configs\\admins_simple.ini"
    }
  }
}