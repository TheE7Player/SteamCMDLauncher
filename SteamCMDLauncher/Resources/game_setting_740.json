// Game Settings for CSGO (740 - Parent 730)
{
  // Key: Tab, Value: Children/Options
  // Hashtag: For translations (Hint to find the correct string for the current language)

  "#General": {
    "Server_Name": {
      "text": "#Server_Name",
      "type": "input",
      "can_leave_blank": true,
      "placeholder": "Counter-Strike: Global Offsensive",
      "width": 300,
      "command": "hostname \"$0\"",
      "command_prefix": "-"
    },
    "Password_SVR": {
      "text": "#Server_Pass",
      "type": "pass",
      "can_leave_blank": true,
      "hint": "#Server_Pass_Hint",
      "width": 200,
      "command": "sv_password $0",
      "command_prefix": "-",
      "alert": "#Server_Pass_Alert",
      "placeholder": "#Server_Pass_PlaceHolder"
    },
    "Server_Tick": {
      "text": "#Is_128_Tick",
      "type": "check",
      "default": false,
      "hint": "#Is_128_Tick_Hint",
      "command": "tickrate $0",
      "command_prefix": "-"
    },
    "Server_Lan": {
      "text": "#Is_Lan",
      "type": "check",
      "default": false,
      "hint": "#Is_Lan_Hint",
      "command": "sv_lan $0",
      "command_prefix": "-"
    }
  }
}