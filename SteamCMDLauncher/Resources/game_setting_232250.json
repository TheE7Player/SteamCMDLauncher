// Game Settings for TF2 (232250 - Parent 440)
{
	"setup": {
		"target": "srcds.exe",
		"precommands": "-console -game tf2 +clientport 27006 -ip $IP4",
		"server_join_command": "connect $IP;password $P;"
	},
	"#General": {
		"Server_Name": {
			"text": "#Server_Name",
			"type": "input",
			"can_leave_blank": true,
			"placeholder": "TF2 Server",
			"width": 300,
			"command": "hostname $",
			"command_prefix": "+"
		},
		"Password_SVR": {
			"text": "#Server_Pass",
			"type": "pass",
			"tag": "PASS",
			"can_leave_blank": true,
			"hint": "#Server_Pass_Hint",
			"width": 200,
			"command": "sv_password $",
			"command_prefix": "+",
			"alert": "#Server_Pass_Alert",
			"placeholder": "#Server_Pass_PlaceHolder"
		},
		"Server-Port": {
			"text": "#ServerPort",
			"type": "combo",
			"tag": "PORT",
			"default": "27015",
			"combo-range": "27010-27030",
			"command": "port $",
			"command_prefix": "-"
		},
		"Server_Vac": {
			"text": "#Is_Vac",
			"type": "check",
			"default": true,
			"hint": "#Is_Vac_Hint",
			"returns": "insecure, _",
			"command": "$",
			"command_prefix": "-"
		},
		"Server_Lan": {
			"text": "#Is_Lan",
			"type": "check",
			"default": false,
			"hint": "#Is_Lan_Hint",
			"returns": "0, 1",
			"command": "sv_lan $",
			"command_prefix": "-"
		}
	},
	"#GameSettings": {
		"Max-Players": {
			"text": "#MaxP",
			"type": "combo",
			"default": "4",
			"combo-range": "1-40",
			"command": "maxplayers $",
			"command_prefix": "+"
		},
		"Enable_Cheats": {
			"text": "#ECheats",
			"type": "check",
			"default": false,
			"hint": "#ECheats_Hint",
			"returns": "0, 1",
			"command": "sv_cheats $",
			"command_prefix": "+"
		},
		"Enable_Voice": {
			"text": "#EVoice",
			"type": "check",
			"default": false,
			"hint": "#EVoice_Hint",
			"returns": "0, 1",
			"command": "sv_alltalk $",
			"command_prefix": "+"
		},
		"Enable_Pause": {
			"text": "#EPause",
			"type": "check",
			"default": false,
			"hint": "#EPause_Hint",
			"returns": "0, 1",
			"command": "sv_pausable $",
			"command_prefix": "+"
		}
	},
	"#MapSettings": {
		"Server_Map": {
			"text": "#Svr-Map",
			"type": "combo",
			"can_leave_blank": false,
			"carry-dir": true,
			"combo-target": "\\tf\\maps;*.bsp",
			"hint": "#Svr-Map-Hint",
			"command": "map $",
			"command_prefix": "+"
		},
		"Time_Limit": {
			"text": "#TLimit",
			"type": "combo",
			"default": "30",
			"combo-range": "0-60",
			"hint": "#TLimit-Hint",
			"command": "mp_timelimit $",
			"command_prefix": "+"
		},
		"Max_Rounds": {
			"text": "#MRounds",
			"type": "combo",
			"default": "10",
			"combo-range": "1-60",
			"hint": "#MRounds-Hint",
			"command": "mp_maxrounds $",
			"command_prefix": "+"
		},
		"Allow_Upload": {
			"text": "#AUpload",
			"type": "check",
			"default": false,
			"hint": "#AUpload_Hint",
			"returns": "0, 1",
			"command": "sv_allowupload $",
			"command_prefix": "+"
		},
		"Allow_Download": {
			"text": "#ADownload",
			"type": "check",
			"default": false,
			"hint": "#ADownload_Hint",
			"returns": "0, 1",
			"command": "sv_allowdownload $",
			"command_prefix": "+"
		}
	}
}